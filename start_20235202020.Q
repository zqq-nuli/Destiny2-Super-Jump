[General]
SyntaxVersion=2
BeginHotkey=121
BeginHotkeyMod=0
PauseHotkey=0
PauseHotkeyMod=0
StopHotkey=123
StopHotkeyMod=0
RunOnce=1
EnableWindow=
MacroID=1be4f54c-8dad-4c8c-945e-50fac1dd267a
Description=超级跳脚本
Enable=1
AutoRun=0
[Repeat]
Type=0
Number=1
[SetupUI]
Type=1
QUI=Form1
[Relative]
SetupOCXFile=
[Comment]

[UIPackage]



[Script]
Event Form1.Button1.Click   
    If Form1.bl1.Value = 1 Then 
        // 冰猎悬崖
        If Form1.blKey1.Virtualkey Then 
            //        Form1.blKey1.Virtualkey = Form1.blKey1.Virtualkey
            Form1.blKey1.Register True 
        Else 
            Form1.blKey1.Register False 
        End If
    End If
    If Form1.bl2.Value = 1 Then 
        // 冰裂平地
        If Form1.blKey2.Virtualkey Then 
            //        Form1.blKey2.Virtualkey = Form1.blKey2.Virtualkey
            Form1.blKey2.Register True 
        Else 
            Form1.blKey2.Register False 
        End If
    End If
    If Form1.hz1.Value = 1 Then 
        // 火中平地
        If Form1.hzKey1.Virtualkey Then 
            //        Form1.hzKey1.Virtualkey = Form1.hzKey1.Virtualkey
            Form1.hzKey1.Register True 
        Else 
            Form1.hzKey1.Register False 
        End If
    End If
    If Form1.hz2.Value = 1 Then 
        // 火中悬崖
        If Form1.hzKey2.Virtualkey Then 
            //        Form1.hzKey2.Virtualkey = Form1.hzKey2.Virtualkey
            Form1.hzKey2.Register True 
        Else 
            Form1.hzKey2.Register False 
        End If
    End If
End Event 
//控制冰猎悬崖
Event Form1.blKey1.Hotkey   
    If Form1.bl1.Value = 1 Then 
        //        Msgbox "触发冰猎悬崖"  
        Form1.blKey1.Register False
        Call bl1()
        Form1.blKey1.Register True
    Else  
        Form1.blKey1.Register False
    End If
End Event 
//控制冰猎平地
Event Form1.blKey2.Hotkey   
    If Form1.bl2.Value = 1 Then 
        //    Msgbox "触发冰猎平地" 
        Form1.blKey2.Register False
        Call bl2()
        Form1.blKey2.Register True
    Else  
        Form1.blKey2.Register False
    End If
End Event 
//控制火中平地
Event Form1.hzKey1.Hotkey   
    If Form1.hz1.Value = 1 Then 
        //    Msgbox "触发火中平地"  
        Form1.hzKey1.Register False
        Call hz1
        Form1.hzKey1.Register True
    Else  
        Form1.hzKey1.Register False
    End If
End Event 
//控制火中悬崖
Event Form1.hzKey2.Hotkey   
    If Form1.hz2.Value = 1 Then 
        //    Msgbox "触发火中悬崖"  
        Form1.hzKey2.Register False
        Call hz2
        Form1.hzKey2.Register True
    Else  
        Form1.hzKey2.Register False
    End If
End Event 
//冰猎悬崖
Function bl1()
    Log "冰猎悬崖"
    KeyPress "3", 1
    Delay 550
    RightClick 1
    Delay 1
    KeyPress "Space", 1
    Delay 1
    KeyPress "X", 1
End Function
//冰猎平地
Function bl2()
    KeyDown "3", 1
    Delay 10
    KeyUp "3", 1
    Delay 550
    KeyDown "Space", 1
    Delay 1
    KeyUp "Space", 1
    Delay 1
    LeftDown 1
    Delay 1
    LeftUp 1
    Delay 1
    KeyDown "Space", 1
    Delay 1
    KeyUp "Space", 1
    Delay 1
    KeyDown "X", 1
    Delay 1
    KeyUp "X", 1
End Function
//火中平地
Sub hz1()
    KeyDown "3", 1
    Delay 10
    KeyUp "3", 1
    Delay 550
    KeyDown "Space", 1
    Delay 1
    KeyUp "Space", 1
    Delay 1
    LeftDown 1
    Delay 1
    LeftUp 1
    Delay 1
    KeyDown "Space", 1
    Delay 1
    KeyUp "Space", 1
    Delay 1
    KeyDown "F", 1
    Delay 1
    KeyUp "F", 1
    Delay 1
    KeyDown "Space", 1
    Delay 1
    KeyUp "Space", 1
End Sub
//火中悬崖
Sub hz2()
    KeyDown "3", 1
    Delay 10
    KeyUp "3", 1
    Delay 550
    RightDown 1
    Delay 1
    RightUp 1
    Delay 1
    KeyDown "Space", 1
    Delay 1
    KeyUp "Space", 1
    Delay 1
    KeyDown "F", 1
    Delay 1
    KeyUp "F", 1
    Delay 1
    KeyDown "Space", 1
    Delay 1
    KeyUp "Space", 1
End Sub
